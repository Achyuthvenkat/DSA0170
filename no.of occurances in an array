#include <iostream>
using namespace std;
int main (){
  int arr[7];
  int size=sizeof(arr)/sizeof(arr[0]);
  int elementTofind=4;
  int count=0,i;
  
  cout<<"Enter a array of numbers:"<<endl;
  for(i=0;i<size;i++){
    cin>>arr[i];
  }
   for(i=0;i<size;i++){
    if( arr[i]==elementTofind)
    
    count++;
  }
  cout<<"element "<<elementTofind<<" occours "<<count<<" times "<<endl;
}
